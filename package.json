{
  "name": "sandbox-electron",
  "version": "0.0.1",
  "author": "gmanPark",
  "description": "this is first app",
  "license": "MIT",
  "main": "./app.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dist": "electron-builder",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port=3000 --disable-host-check=true",
    "web:build": "ng build --prod --aot --build-optimizer --output-hashing none --named-chunks true --bh .",
    "electron:dev": "npm run start && gulp",
    "electron:prod": "npm run build:prod && electron ./dist",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:windows": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "electron:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "make:mac": "electron-packager ./ --platform=darwin --arch=x64 --out=./build",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "package-mac": "electron-packager . myApp --overwrite --asar=true --platform=darwin --arch=x64 --icon=src/favicon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/favicon.ico --prune=true --out=release-builds --version-string.CompanyName='CE' --version-string.FileDescription='CE' --version-string.ProductName='ElectronTutorialApp'",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=src/favicon.ico --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/sandbox-electron-app-darwin-x64/sandbox-electron-app.app sandbox-electron-app --out=release-builds --overwrite"
  },
  "optionalDependencies": {
    "7zip-bin-mac": "^1.x.x"
  },
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "core-js": "^2.5.1",
    "electron-reload": "^1.2.2",
    "gulp": "^3.9.1",
    "ngx-electron": "^1.0.3",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.4.2",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/language-service": "^4.4.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "autoprefixer": "^6.5.3",
    "circular-dependency-plugin": "^3.0.0",
    "codelyzer": "~3.1.1",
    "copy-webpack-plugin": "^4.1.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.35.1",
    "electron-connect": "^0.6.2",
    "electron-winstaller": "^2.6.3",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.5",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^5.1.2",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "url-loader": "^0.5.7",
    "webpack": "~3.5.5",
    "webpack-concat-plugin": "1.4.0",
    "webpack-dev-server": "~2.7.1"
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    },
    "nsis": {
      "oneClick": false,
      "runAfterFinish": false
    }
  }
}
